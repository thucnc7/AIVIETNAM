import java.util.Scanner;
public class NumberAnalysis
{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int x = input.nextInt();
        for(int i = 2 ; i*i <= n ; i++){
            if(x % i == 0){
                while(x % i == 0){
                    System.out.printf("%d ",i);
                    x /= i ;
                }
            }
        }
    }
}